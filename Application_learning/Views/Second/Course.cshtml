
@{
    Layout = null;
    var CourseItem = ViewBag.CourseDetails;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Course</title>
</head>
<body>
    <div>
        <h2>Course Detailes</h2>
        <ul>
            <li>@CourseItem.Title</li>
            <li>@CourseItem.StudentEnrolled</li>
            <li>@CourseItem.Rating</li>
            <li>@CourseItem.Fees</li>
        </ul>
        
    </div>
</body>
</html>
